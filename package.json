{
  "name": "angular1-ts-webpack",
  "version": "0.1.0",
  "description": "A seed project for an angular 1.x application with TypeScript and webpack support.",
  "main": "/",
  "scripts": {
    "start": "npm run server",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "postinstall": "typings install"
  },
  "author": "Andre Giannico <andre@andregiannico.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.6",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.6",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "ng-annotate-loader": "^0.1.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typings": "^1.0.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}